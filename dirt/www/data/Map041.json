{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":213,"indent":0,"parameters":[-1,8,true]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["那是一间狭小，决不能称为普通的卧室。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["给人怪异感觉的是，墙上挂满的各色各样的面具。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["但是仔细看就会发现，面具大多是硬纸壳做的。"]},{"code":401,"indent":0,"parameters":["上面的脸也终究是小孩子的涂鸦。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["一个孩子，在书桌旁忙碌着。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["在画着什么。"]},{"code":401,"indent":0,"parameters":["人脸。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["虽然只是涂鸦，看起来像是一个凶恶的男人。"]},{"code":401,"indent":0,"parameters":["那是认识的人，还是想要成为的人？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["手边摆着切得乱七八糟的硬纸板。"]},{"code":401,"indent":0,"parameters":["孩子戴着眼镜，脸上，手上贴着绷带。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["在绷带不能完全覆盖的地方露出淤青。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["桌子边上立着一张照片，一个年幼的女孩，"]},{"code":401,"indent":0,"parameters":["拉着她的双亲，可是双亲的脸却被马克笔涂黑。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["一本摊开的童话书的扉页上歪扭地写着——rune。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["你愤怒吗？"]},{"code":401,"indent":0,"parameters":["你想要报复他们吗？"]},{"code":401,"indent":0,"parameters":["你想要的东西永远得不到了吗？"]},{"code":401,"indent":0,"parameters":["所以你才要追求那最荒谬的东西吗？"]},{"code":205,"indent":0,"parameters":[-1,{"list":[{"code":4,"indent":null},{"code":17,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":4,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":17,"indent":null}]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["我摸了摸口袋中的面具。"]},{"code":401,"indent":0,"parameters":["犹豫了一会儿，掏出来，把它放在女孩的床上。"]},{"code":121,"indent":0,"parameters":[71,71,0]},{"code":213,"indent":0,"parameters":[-1,8,true]},{"code":205,"indent":0,"parameters":[-1,{"list":[{"code":1,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":1,"indent":null}]},{"code":211,"indent":0,"parameters":[0]},{"code":213,"indent":0,"parameters":[1,8,true]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":2,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":2,"indent":null}]},{"code":213,"indent":0,"parameters":[1,2,true]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":1,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":1,"indent":null}]},{"code":213,"indent":0,"parameters":[1,8,true]},{"code":201,"indent":0,"parameters":[0,42,0,4,6,0]},{"code":123,"indent":0,"parameters":["A",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":2,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":1,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directi