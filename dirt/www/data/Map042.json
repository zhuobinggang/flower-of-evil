{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":223,"indent":0,"parameters":[[0,0,0,0],60,true]},{"code":211,"indent":0,"parameters":[0]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["永恒的圣火熄灭，"]},{"code":401,"indent":0,"parameters":["这个世界迎来短暂的黑暗。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["在那短暂的黑暗中，人们迎来了新的神。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["后人称之为“晦暗的面具之神”。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["它不再寄身于祭司一族的神龛之中，"]},{"code":401,"indent":0,"parameters":["而是将自己的灵散播到世界的每一个角落。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["……"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["当那辆神秘的马车行驶在幽静的林间小径，"]},{"code":401,"indent":0,"parameters":["有时她会落到车中，没有人的那边坐椅上。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["她会讲述那些新解明的，关于这个世界的真相。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["我们无法触及‘他们’，但是能够根据创世的痕迹，"]},{"code":401,"indent":0,"parameters":["获得这个世界的‘元知识’。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["只要持之以恒地努力，"]},{"code":401,"indent":0,"parameters":["终有一天能够抵达最为接近‘他们’的地方。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["马夫时常沉默，有时也会回几句不知所谓的话。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["“蓝色的红，”马夫说，“你能造出来吗？蓝色的红。”"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["就算是我，也要接受这个世界的规则。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【马夫】"]},{"code":401,"indent":0,"parameters":["那你说，'他们'呢？‘他们’的世界中会有蓝色的红吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["大概也不会有吧。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【马夫】"]},{"code":401,"indent":0,"parameters":["那‘他们’和我们又有什么不一样呢？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["……"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["新神感到无语，"]},{"code":401,"indent":0,"parameters":["便像马车上的另一位听众那样，"]},{"code":401,"indent":0,"parameters":["长久地保持沉默。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["过了一会儿，她会说，"]},{"code":401,"indent":0,"parameters":["”我得走了，我很忙的。“"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["“哦，不送。”马夫这样说了。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["你可以表示得更加不舍一点，"]},{"code":401,"indent":0,"parameters":["或许我会给你更多情报。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【马夫】"]},{"code":401,"indent":0,"parameters":["你爱给不给。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【马夫】"]},{"code":401,"indent":0,"parameters":["反正我们最不缺的就是时间。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["戴着面具的神笑了。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["神会祝福你们。"]},{"code":401,"indent":0,"parameters":["再见了——"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["【？？？】"]},{"code":401,"indent":0,"parameters":["——遥远过去的英雄，还有破碎的旧世界之神。"]},{"code":221,"indent":0,"parameters":[]},{"code":105,"indent":0,"parameters":[2,false]},{"code":405,"indent":0,"parameters":["TRUE END"]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["脚本：海边的taku"]},{"code":405,"indent":0,"parameters":["音乐：海边的taku"]},{"code":405,"indent":0,"parameters":["美工：海边的taku"]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["blibili关注taku的交错电台谢谢喵。"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["TRUE END —— 找寻灵魂的旅程"]},{"code":354,"indent":0,"parameters":[]},{"code":123,"indent":0,"parameters":["A",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":2,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":1,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":0,"y":0},
